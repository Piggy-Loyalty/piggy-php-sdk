  "require": {
    "php": ">=8.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~5.3 || ~6.0 || ^7.0",
    "marc-mabe/php-enum": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^6.5 || ^7.1 || ^8.5",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit tests"
  }
}
